<div class="container">
  <div class="row">
    <% @songs.each do |song| %>
    <div class="col-sm-6 col-md-3">
      <div class="thumbnail">
        <img src="https://upload.wikimedia.org/wikipedia/commons/2/26/Beats_Music_logo.svg" height="50" width="50">
        <div class="caption text-center">
          <h4><%=song.title%></h4>
        </div>
      </div>
    </div>
    <%end%>
  </div>
</div>
